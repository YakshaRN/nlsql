            "method": "POST",
            "url": "{{base_url}}/query",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\"question\": \"What is the P01 extreme cold temperature forecast for RTO for the next 10 days starting 2026-01-20 00:00?\", \"session_id\": \"{{session_id}}\"}"
            }
          }
        },
        {
