        {
          "name": "1.1 GSI Peak Probability (complete)",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/query",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\"question\": \"What is the peak probability of GSI exceeding 0.60 over the next 14 days starting from 2026-01-15 12:00?\", \"session_id\": \"{{session_id}}\"}"
            }
          }
        },
        {
          "name": "1.2 Tightest Hour GSI",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/query",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\"question\": \"Show me the tightest hour - the hour with highest average GSI - starting from 2026-01-15 12:00\", \"session_id\": \"{{session_id}}\"}"
            }
          }
        },
        {
          "name": "1.3 P01 Extreme Cold Temp",
